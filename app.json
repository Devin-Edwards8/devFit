{
  "expo": {
    "name": "fitness_app",
    "slug": "fitness_app",
    "privacy": "public",
    "description": "An app to assist with your fitness goals. Tracks your workouts and daily nutritional intake. ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "iosfitnessapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.anonymous.fitness_app"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    }
  }
}
